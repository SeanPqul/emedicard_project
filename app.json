{
  "expo": {
    "name": "emedicard_project",
    "slug": "emedicard_project",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "emedicardproject",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "deepLinking": {
      "enabled": true,
      "prefixes": [
        "emedicardproject://",
        "https://emedicard.app"
      ]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.emedicard.project",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}

    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "backend": "workspace:*",
    "convex": "^1.27.3",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^17.2.1",
    "expo": "54.0.12",

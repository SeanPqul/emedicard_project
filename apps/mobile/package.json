{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "cd ../../backend && npx convex codegen",
    "dev": "expo start --dev-client",
    "start": "expo start --dev-client",
    "build": "expo export --platform web",
    "typecheck": "tsc --noEmit",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=src/.*\\.test\\.(ts|tsx)$",
    "test:integration": "jest --testPathPattern=__tests__/integration/.*\\.test\\.(ts|tsx)$",
    "test:verbose": "jest --verbose",
    "test:update-snapshots": "jest -u",
    "test:clear-cache": "jest --clearCache",
    "clean": "rm -rf .expo dist web-build",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "seed:seedJobCategoriesAndRequirements": "npx convex run --dir ../backend/convex seed:seedJobCategoriesAndRequirements",
    "seed:clearSeedData": "npx convex run --dir ../backend/convex seed:clearSeedData",
    "codemod:convex-rename": "node ./scripts/run-convex-codemod.js"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.13.2",

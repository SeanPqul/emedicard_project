{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "typeRoots": ["src/types", "./node_modules/@types"]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/index.tsx", 
    "app/(auth)/sign-in.tsx",
    "src/types/**/*.d.ts"
, "assets/styles/auth-styles/sign-in.js", "assets/styles/auth-styles/sign-up.js"  ]
}
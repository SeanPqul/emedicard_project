{
  "name": "emedicard_project",
  "private": true,
  "version": "1.0.0",
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "test": "turbo run test",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "ocr:start": "pnpm --filter ocr-service start",
    "ocr:dev": "pnpm --filter ocr-service dev",
    "ocr:build": "pnpm --filter ocr-service build"
  },
  "devDependencies": {
    "turbo": "latest"
  },
  "pnpm": {
    "allowedScriptVersions": {
      "@clerk/shared": "*",
      "@tailwindcss/oxide": "*",
      "browser-tabs-lock": "*",
      "core-js": "*",
      "esbuild": "*",
      "sharp": "*",
      "unrs-resolver": "*"
    },
    "overrides": {
      "expo-web-browser": "15.0.8"
    },
    "onlyBuiltDependencies": [
      "@clerk/shared",
      "@tailwindcss/oxide",
      "browser-tabs-lock",
      "core-js",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  },
  "dependencies": {
    "expo": "~54.0.12",
    "react": "19.1.0",
    "react-native": "0.81.4"
  }
}
